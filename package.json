{
  "name": "modrinth",
  "description": "JavaScript library for accessing the Modrinth API",
  "author": "rubybb <https://github.com/rubybb>",
  "version": "1.2.2",
  "types": "src/index.d.ts",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm ./lib ./docs -rf",
    "dev:build": "tsc",
    "dev:docs": "pnpx typedoc",
    "dev:serve": "pnpm build:docs && pnpx lite-server --baseDir=\"docs\"",
    "prod:build": "tsc && npx typedoc && npm publish",
    "prod:serve": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubybb/modrinth.git"
  },
  "keywords": [
    "modrinth",
    "api"
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/rubybb/modrinth/issues"
  },
  "homepage": "https://modrinth.js.org",
  "dependencies": {
    "@rubybb/http": "^1.6.0",
    "deepmerge": "^4.2.2",
    "form-data": "^3.0.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@convergencelabs/typedoc-plugin-custom-modules": "^0.4.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0",
    "typedoc": "^0.20.24",
    "typescript": "^4.1.3"
  }
}
